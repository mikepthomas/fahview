<project xmlns="http://maven.apache.org/DECORATION/1.3.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.3.0 http://maven.apache.org/xsd/decoration-1.3.0.xsd">

    <body>
        <breadcrumbs>
            <item name="${project.name}" href="${project.url}"/>
        </breadcrumbs>

        <links>
            <item name="Google Code" href="http://fahview.googlecode.com/"/>
        </links>

        <menu ref="parent" />

        <menu title="Project Report">
            <item name="Abstract" href="abstract.html"/>
            <item name="List of Figures" href="figures.html"/>
            <item name="Report" href="report.html"/>

            <item name="Introduction" href="index.html"/>
            <item name="Problems and Opportunities" href="problem.html"/>
            <item name="Solution" href="solution.html"/>
            <item name="Aim and Objectives" href="objectives.html"/>
            <item name="Development Method" href="development.html"/>
            <item name="Project Planning" href="planning.html"/>
            <item name="Research" href="research.html"/>
        </menu>

        <menu ref="reports" />
    </body>

    <publishDate position="right"/>
    <version position="right"/>

    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>

    <custom>
        <fluidoSkin>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>

            <leftColumnClass>span3</leftColumnClass>
            <bodyColumnClass>span9</bodyColumnClass>

            <navBarStyle>navbar-inverse</navBarStyle>

            <sideBarEnabled>true</sideBarEnabled>
            <topBarEnabled>true</topBarEnabled>
            <topBarIcon>
                <name>${project.name}</name>
                <href>${project.url}</href>
            </topBarIcon>
        </fluidoSkin>
    </custom>

</project>
